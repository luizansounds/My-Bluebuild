#!/usr/bin/env sh
spotifyd -u elatetunic@gmail.com -d PC-DAEMON --device-type speaker --volume-normalisation --autoplay --password lblue135-

rm -rf $HOME/.config/vivaldi/SingletonCookie $HOME/.config/vivaldi/SingletonLock $HOME/.config/vivaldi/SingletonSocket

pw-loopback --capture-props='node.target=youroutput' --playback-props='media.class=Audio/Source node.name=virtmic node.description="audio-loopback(use qpwgraph)"'
